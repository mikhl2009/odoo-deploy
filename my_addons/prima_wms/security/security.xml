<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="module_category_prima_wms" model="ir.module.category">
        <field name="name">Prima WMS</field>
        <field name="sequence">50</field>
    </record>

    <!-- WMS Användare -->
    <record id="group_prima_wms_user" model="res.groups">
        <field name="name">WMS Användare</field>
        <field name="category_id" ref="module_category_prima_wms"/>
        <field name="implied_ids" eval="[(4, ref('stock.group_stock_user'))]"/>
    </record>

    <!-- WMS Manager -->
    <record id="group_prima_wms_manager" model="res.groups">
        <field name="name">WMS Manager</field>
        <field name="category_id" ref="module_category_prima_wms"/>
        <field name="implied_ids" eval="[(4, ref('group_prima_wms_user'))]"/>
    </record>
</odoo>
